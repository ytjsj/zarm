{
  "name": "zarm",
  "version": "1.2.0-beta.21",
  "description": "基于 React 的移动端UI库",
  "tags": [
    "zarm",
    "zarm-ui",
    "react",
    "react-zarm",
    "zhongan"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "zarm",
    "zarm-ui",
    "zarm-mobile",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "mobile"
  ],
  "license": "MIT",
  "repository": "git://github.com/ZhonganTechENG/zarm",
  "bugs": {
    "url": "https://github.com/ZhonganTechENG/zarm/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "npm run deploy && npm run dev",
    "clean": "rm -rf lib es dist .temp",
    "dev": "webpack-dev-server --config scripts/webpack/config.dev.js",
    "rn-start": "node node_modules/react-native/local-cli/cli.js start",
    "rn-watch": "tsc --module es6 --jsx react-native --w",
    "ios": "concurrently \"npm run rn-watch\" \"node node_modules/react-native/local-cli/cli.js run-ios --project-path examples-rn/ios\" \"npm run rn-start\"",
    "android": "concurrently \"npm run rn-watch\" \"node node_modules/react-native/local-cli/cli.js run-android --root examples-rn\" \"npm run rn-start\"",
    "build:es": "tsc --outDir es -d",
    "build:lib": "tsc --outDir lib -d && tsc --outDir .temp --jsx preserve && babel .temp --out-dir lib --presets=env,react,stage-0 && rm -rf .temp",
    "build:umd": "webpack --config scripts/webpack/config.umd.js && NODE_ENV=production webpack --config scripts/webpack/config.umd.js",
    "build:css": "cd scripts/gulp && gulp",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:css",
    "site:build": "rm -rf assets && webpack --config scripts/webpack/config.site.js",
    "site": "npm run site:build && gh-pages -d assets",
    "lint:ts": "tslint -c tslint.json 'components/**/*.{ts,tsx}'",
    "lint:js": "eslint examples examples-rn --ext '.js,.jsx'",
    "lint:style": "stylelint styles/**/*.scss examples/styles/**/*.scss --syntax scss",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:style",
    "lint-staged": "lint-staged",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docs": "cd docs && gitbook install && gitbook serve",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "zscroller": "^0.2.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/jest": "^21.1.5",
    "@types/react": "^16.0.19",
    "@types/react-dom": "15.5.1",
    "@types/react-native": "^0.51.9",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.2.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "enzyme-to-json": "^3.2.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fastclick": "^1.0.6",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-filesize": "0.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-loader": "^1.3.3",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.52.0",
    "react-navigation": "^1.0.0-beta.23",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "^20.0.7",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "typescript": "2.6.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.7.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/components"
    ],
    "setupFiles": [
      "<rootDir>/scripts/jest/environment.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest/setup.js",
    "testRegex": "/__tests__/.*(\\.jsx|[^d]\\.ts)$",
    "collectCoverageFrom": [
      "components/**/*.{ts,tsx}",
      "!components/*/*.native.{ts,tsx}",
      "!components/*/PropsType.{ts,tsx}",
      "!components/**/style/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/scripts/jest/preprocessor",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "components/**/*.{ts,tsx}": "lint:ts",
    "{components,examples,examples-rn}/**/*.{js,jsx}": "lint:js",
    "{styles,examples}/**/*.scss": "lint:style"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
